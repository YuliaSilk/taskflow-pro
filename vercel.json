{
    "version": 2,
    "builds": [
      {
        "src": "src/index.tsx",
        "use": "@vercel/static-build"
      },
      {
        "src": "server/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "src/index.tsx"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }